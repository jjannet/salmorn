<!DOCTYPE html>
<html ng-app="appwar">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <!--Plugins css-->
    <link rel="stylesheet" href="~/Content/css/plugins.css">
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css" />

    <!--Custom Styles-->
    <link rel="stylesheet" href="~/Content/css/style.css">
</head>
<body>
    <div class="loader">
        <div class="loader-inner">
            <div class="loader-blocks">
                <span class="block-1"></span>
                <span class="block-2"></span>
                <span class="block-3"></span>
                <span class="block-4"></span>
                <span class="block-5"></span>
                <span class="block-6"></span>
                <span class="block-7"></span>
                <span class="block-8"></span>
                <span class="block-9"></span>
                <span class="block-10"></span>
                <span class="block-11"></span>
                <span class="block-12"></span>
                <span class="block-13"></span>
                <span class="block-14"></span>
                <span class="block-15"></span>
                <span class="block-16"></span>
            </div>
        </div>
    </div>
    <!--PreLoader Ends-->
    <!-- header -->
    <header class="site-header just-sidemenu">

        <!--side menu open button-->
        <a href="javascript:void(0)" class="d-lg-inline-block sidemenu_btn" id="sidemenu_toggle">
            <span></span> <span></span> <span></span>
        </a>

        <!-- side menu -->
        <div class="side-menu">
            <div class="inner-wrapper">
                <span class="btn-close" id="btn_sideNavClose"><i></i><i></i></span>
                <nav class="side-nav">
                    <a class="navbar-brand" href="index.html">
                        <img src="~/Content/images/logo-transparent.png" alt="logo">
                    </a>
                    <ul class="navbar-nav w-100">
                        <li class="nav-item active">
                            <a class="nav-link" href="#banner-main">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#our-process">Features</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#our-team">Team</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#portfolio_top">Portfolio</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#our-pricings">Packages</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#our-testimonial">Clients</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#our-blog">Blog</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#contactus">contact</a>
                        </li>
                    </ul>
                </nav>

                <div class="side-footer w-100">
                    <ul class="social-icons-simple white top40">
                        <li><a href="javascript:void(0)"><i class="fa fa-facebook"></i> </a> </li>
                        <li><a href="javascript:void(0)"><i class="fa fa-instagram"></i> </a> </li>
                        <li><a href="javascript:void(0)"><i class="fa fa-twitter"></i> </a> </li>
                    </ul>
                    <p class="whitecolor">&copy; 2018 XeOne. Made With Love by themesindustry</p>
                </div>

            </div>
        </div>
        <a id="close_side_menu" href="javascript:void(0);"></a>
        <!-- End side menu -->
    </header>
    <!-- header -->
    @RenderBody()
    <!--Site Footer Here-->
    <footer id="site-footer" class="padding_half">
        <div class="container">
            <div class="row">
                <div class="col-md-12 col-sm-12 text-center">
                    <ul class="social-icons bottom25 wow fadeInUp" data-wow-delay="300ms">
                        <li><a href="javascript:void(0)"><i class="fa fa-facebook"></i> </a> </li>
                        <li><a href="javascript:void(0)"><i class="fa fa-twitter"></i> </a> </li>
                        <li><a href="javascript:void(0)"><i class="fa fa-google-plus"></i> </a> </li>
                        <li><a href="javascript:void(0)"><i class="fa fa-linkedin"></i> </a> </li>
                        <li><a href="javascript:void(0)"><i class="fa fa-instagram"></i> </a> </li>
                        <li><a href="javascript:void(0)"><i class="fa fa-envelope-o"></i> </a> </li>
                    </ul>
                    <p class="copyrights wow fadeInUp" data-wow-delay="400ms"> &copy; 2018 XeOne. made with love by <a href="#.">themesindustry</a> </p>
                </div>
            </div>
        </div>
    </footer>
    <!--Footer ends-->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="~/Content/js/jquery-3.1.1.min.js"></script>
    <script src="~/Content/js/angular.min.js"></script>

    <!--Bootstrap Core-->
    <script src="~/Content/js/popper.min.js"></script>
    <script src="~/Content/js/bootstrap.min.js"></script>

    <!--to view items on reach-->
    <script src="~/Content/js/jquery.appear.js"></script>

    <!--Equal-Heights-->
    <script src="~/Content/js/jquery.matchHeight-min.js"></script>

    <!--Owl Slider-->
    <script src="~/Content/js/owl.carousel.min.js"></script>

    <!--number counters-->
    <script src="~/Content/js/jquery-countTo.js"></script>

    <!--Parallax Background-->
    <script src="~/Content/js/parallaxie.js"></script>

    <!--Cubefolio Gallery-->
    <script src="~/Content/js/jquery.cubeportfolio.min.js"></script>

    <!--FancyBox popup-->
    <script src="~/Content/js/jquery.fancybox.min.js"></script>

    <!-- Video Background-->
    <script src="~/Content/js/jquery.background-video.js"></script>

    <!--TypeWriter-->
    <script src="~/Content/js/typewriter.js"></script>

    <!--Particles-->
    <script src="~/Content/js/particles.min.js"></script>

    <!--WOw animations-->
    <script src="~/Content/js/wow.min.js"></script>

    <!--Revolution SLider-->
    <script src="~/Content/js/revolution/jquery.themepunch.tools.min.js"></script>
    <script src="~/Content/js/revolution/jquery.themepunch.revolution.min.js"></script>
    <!-- SLIDER REVOLUTION 5.0 EXTENSIONS  (Load Extensions only on Local File Systems !  The following part can be removed on Server for On Demand Loading) -->
    <script src="~/Content/js/revolution/extensions/revolution.extension.actions.min.js"></script>
    <script src="~/Content/js/revolution/extensions/revolution.extension.carousel.min.js"></script>
    <script src="~/Content/js/revolution/extensions/revolution.extension.kenburn.min.js"></script>
    <script src="~/Content/js/revolution/extensions/revolution.extension.layeranimation.min.js"></script>
    <script src="~/Content/js/revolution/extensions/revolution.extension.migration.min.js"></script>
    <script src="~/Content/js/revolution/extensions/revolution.extension.navigation.min.js"></script>
    <script src="~/Content/js/revolution/extensions/revolution.extension.parallax.min.js"></script>
    <script src="~/Content/js/revolution/extensions/revolution.extension.slideanims.min.js"></script>
    <script src="~/Content/js/revolution/extensions/revolution.extension.video.min.js"></script>

    <script type="text/javascript" src="~/Content/js/site.js"></script>

    <!--Google Api-->
    @*<script src="~/Content/https://maps.googleapis.com/maps/api/js?key=AIzaSyA_egfOLjahHB0IWpykRZrVFD8fN4JMgmw"></script>*@
    <script src="~/Content/js/functions.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
